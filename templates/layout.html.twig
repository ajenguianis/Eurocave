<!DOCTYPE html>
<html lang="fr">
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>{% block title %}EuroCave{% endblock %}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <meta name="MobileOptimized" content="320">
    {% block stylesheets %}
        <!-- BEGIN GLOBAL MANDATORY STYLES -->
        <link href="{{ asset('assets/plugins/css/font-awesome/font-awesome.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('assets/plugins/css/bootstrap/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('assets/scripts/css/custom.css') }}" rel="stylesheet" type="text/css"/>
        <!-- END THEME STYLES -->
    {% endblock %}
</head>
<body>
{% block nav %}
    {{ include('_partials/nav.html.twig') }}
{% endblock %}
{% block body %}
{% endblock %}
<!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
{% block javascripts %}
    <script src="{{ asset('assets/plugins/js/jquery/jquery-3.5.1.slim.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/js/jquery/jquery.form.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/js/popper/popper.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/js/bootstrap/bootstrap.min.js') }}" type="text/javascript"></script>

{% endblock %}
{% block pagejavascripts %}
    <script src="{{ asset('assets/plugins/js/jquery/jquery-3.5.1.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/scripts/js/app.js') }}"></script>

{% endblock %}
{% block readyjavascripts %}
{% endblock %}
<!-- END JAVASCRIPTS -->
</body>
<!-- END BODY -->
</html>
